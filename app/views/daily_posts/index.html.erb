<p id="notice"><%= notice %></p>

<h1>Daily Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Completed</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_posts.each do |daily_post| %>
      <tr>
        <td><%= daily_post.title %></td>
        <td><%= daily_post.completed %></td>
        <td><%= daily_post.user_id %></td>
        <td><%= link_to 'Show', daily_post %></td>
        <td><%= link_to 'Edit', edit_daily_post_path(daily_post) %></td>
        <td><%= link_to 'Destroy', daily_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily Post', new_daily_post_path %>
